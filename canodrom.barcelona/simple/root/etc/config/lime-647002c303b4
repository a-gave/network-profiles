config lime system
        option hostname 'device35'

config lime network
	option main_ipv4_address '10.0.0.35/24'

config generic_uci_config location_coord
	list uci_set 'location.settings.node_latitude=41.42735'
	list uci_set 'location.settings.node_longitude=2.18335'

config net 'lm_hwd_openwrt_wan'
	option autogenerated 'false'
	option linux_name 'eth0.2'
	list protocols 'static'
	option static_ipv4 '192.168.1.35/24'
	option static_ipv6 '2a00:1508:0a00::35/64'
