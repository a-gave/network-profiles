config lime system
        option hostname 'device20'

config lime network
	option main_ipv4_address '10.0.0.20/16'

config generic_uci_config location_coord
	list uci_set 'location.settings.node_latitude=41.42720'
	list uci_set 'location.settings.node_longitude=2.18320'

config net 'lm_hwd_openwrt_wan'
	option autogenerated 'false'
	option linux_name 'eth0.2'
	list protocols 'static'
	option static_ipv4 '192.168.1.20/24'
	option static_ipv6 '2a00:1508:0a00::20/64'
