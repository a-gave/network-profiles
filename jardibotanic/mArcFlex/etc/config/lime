config net 'lm_hwd_openwrt_wan'
	option comment 'in Jardi botanic topology we need to use as wan the inderface which get POE input, while OpenWrt would defalut to lan'
	option autogenerated 'false'
	list protocols 'ieee80211s'
	list protocols 'adhoc'
	list protocols 'batadv:%N1'
	list protocols 'bmx6:0:8021q:_qmp'
	list protocols 'bmx6:0'
	list protocols 'wan'
	option linux_name 'eth0.1'
