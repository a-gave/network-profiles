config lime system
    option hostname BCNmtj3InfoParet

config net 'lm_hwd_openwrt_wan'
	option comment "in Jardi botanic topology we need to use as wan the interface which get POE input, while OpenWrt would default to lan"
	option autogenerated 'false'
	list protocols 'ieee80211s'
	list protocols 'batadv:%N1'
	list protocols 'bmx6:0'
	list protocols 'wan'
	option linux_name 'eth0.1'
